<BCPatient_bc_clinical_notes_HUPHM_relapse_progression>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/reproductive_information.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:BCPatient
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasGravidity;
		rr:objectMap [
			rr:parentTriplesMap <Gravidity_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasParity;
		rr:objectMap [
			rr:parentTriplesMap <Parity_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasMenopauseAge;
		rr:objectMap [
			rr:parentTriplesMap <MenopauseAge_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:oralContraceptiveEndingDate;
		rr:objectMap [
			rml:reference "pillStopDate";
			rr:datatype xsd:date
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasMenarcheAge;
		rr:objectMap [
			rr:parentTriplesMap <MenarcheAge_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasMenopauseStage;
		rr:objectMap [
			rr:parentTriplesMap <MenopauseStage_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:usesContraceptive;
		rr:objectMap [
			rr:parentTriplesMap <UnknownContraceptive_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:usesContraceptive;
		rr:objectMap [
			rr:parentTriplesMap <OralContraceptive_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:usesContraceptive;
		rr:objectMap [
			rr:parentTriplesMap <HormoneTherapyContraceptive_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:usesContraceptive;
		rr:objectMap [
			rr:parentTriplesMap <NoContraceptive_bc_clinical_notes_HUPHM_relapse_progression>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:usesContraceptive;
		rr:objectMap [
			rr:parentTriplesMap <OtherContraceptive_bc_clinical_notes_HUPHM_relapse_progression>
		]
	].

<Gravidity_bc_clinical_notes_HUPHM_relapse_progression>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/reproductive_information.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{numberPregnancies}";
