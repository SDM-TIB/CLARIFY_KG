@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<BCPatient_bc_clinical_notes_HUPHM_oncological_history>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:BCPatient
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_previousCancerHistory_OrNot;
		rr:objectMap [
			rr:parentTriplesMap <has_previous_cancers_bc_clinical_notes_HUPHM_oncological_history>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasPreviousCancerType;
		rr:objectMap [
			rr:parentTriplesMap <previous_cancer_type_bc_clinical_notes_HUPHM_oncological_history>
		]
	];					
	rr:predicateObjectMap [
		rr:predicate clarify:previousCancerTypeDescription;
		rr:objectMap [
			rml:reference "previous_cancer_description";
		]
	].	

<has_previous_cancers_bc_clinical_notes_HUPHM_oncological_history>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <has_previous_cancers_Function_bc_clinical_notes_HUPHM_oncological_history> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/PatientCancerHistory";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "has_previous_cancers";		
		]
	].

<previous_cancer_type_bc_clinical_notes_HUPHM_oncological_history>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <previous_cancer_type_Function_bc_clinical_notes_HUPHM_oncological_history> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/CancerType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "previous_cancer_type";		
		]
	].

