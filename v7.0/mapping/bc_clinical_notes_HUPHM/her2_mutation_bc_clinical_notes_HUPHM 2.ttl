@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<BCPatient_bc_clinical_notes_HUPHM_her2_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:BCPatient
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <Biomarker_bc_clinical_notes_HUPHM_her2_mutation>
		]
	].	

<Biomarker_bc_clinical_notes_HUPHM_her2_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:constant "http://clarify2020.eu/entity/HER2";
        rr:termType rr:IRI;
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/Biomarker";
        ]
    ];  			
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestIHQResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerIHQResult_bc_clinical_notes_HUPHM_her2_mutation>		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestFISHResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerFISHResult_bc_clinical_notes_HUPHM_her2_mutation>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_performed_study;
		rr:objectMap [
			rr:parentTriplesMap <study_performed_bc_clinical_notes_HUPHM_her2_mutation>		
		]
	].

<BiomarkerIHQResult_bc_clinical_notes_HUPHM_her2_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <BiomarkerIHQResult_Function_bc_clinical_notes_HUPHM_her2_mutation> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/BiomarkerTestResult";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "result_her2_ihq";		
		]
	].

<BiomarkerFISHResult_bc_clinical_notes_HUPHM_her2_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{result_her2_fish}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/BiomarkerTestResult";      
        ]
    ].

<study_performed_bc_clinical_notes_HUPHM_her2_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{study_performed}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/PerformedStudyStatus";      
        ]
    ].	

#######################################
############## Functions ##############
#######################################

<BiomarkerIHQResult_Function_bc_clinical_notes_HUPHM_her2_mutation>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/her2_mutation.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic_HUPHM 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "her2_mutation" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "result_her2_ihq" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/entity/" 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "result_her2_ihq" 
            ]
        ];                         
    ].


