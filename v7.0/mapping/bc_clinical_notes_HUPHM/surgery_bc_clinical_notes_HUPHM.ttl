<BCPatient_bc_clinical_notes_HUPHM_surgery>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:BCPatient
	];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_TumorSize;
        rr:objectMap [
            rr:parentTriplesMap <TumorSize_bc_clinical_notes_HUPHM_surgery>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_LymphNodesInvolvement;
        rr:objectMap [
            rr:parentTriplesMap <LymphNodesInvolvement_bc_clinical_notes_HUPHM_surgery>     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_MetastatisStage;
        rr:objectMap [
            rr:parentTriplesMap <MetastatisStage_bc_clinical_notes_HUPHM_surgery>
        ]
    ];  
	rr:predicateObjectMap [
		rr:predicate clarify:hasOncologicalSurgery;
		rr:objectMap [
			rr:parentTriplesMap <OncologicalSurgery_bc_clinical_notes_HUPHM_surgery>		
		]
	].	

<OncologicalSurgery_bc_clinical_notes_HUPHM_surgery>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}_OncologicalSurgery_{surgery_date}";
		rr:class clarify:OncologicalSurgery
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasPathologicalResponse;
		rr:objectMap [
			rr:parentTriplesMap <SurgeryPathologicalResponse_bc_clinical_notes_HUPHM_surgery>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasResectionGrade;
		rr:objectMap [
			rr:parentTriplesMap <SurgeryResectionDegree_bc_clinical_notes_HUPHM_surgery>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_PerioperativeDeath_orNot;
		rr:objectMap [
			rr:parentTriplesMap <PerioperativeDeath_bc_clinical_notes_HUPHM_surgery>
		]
	];		
	rr:predicateObjectMap [
		rr:predicate clarify:date;
		rr:objectMap [
			rml:reference "surgery_date";
			rr:datatype xsd:date;
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasType;
		rr:objectMap [
			rr:parentTriplesMap <SurgeryType_bc_clinical_notes_HUPHM_surgery>
		]
	];		
	rr:predicateObjectMap [
		rr:predicate clarify:hasClass;
		rr:objectMap [
			rr:parentTriplesMap <SurgeryClass_bc_clinical_notes_HUPHM_surgery>
		]
	].
		

<SurgeryPathologicalResponse_bc_clinical_notes_HUPHM_surgery>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <SurgeryPathologicalResponse_Function_bc_clinical_notes_HUPHM_surgery> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/SurgeryPathologicalResponse";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "pathological_response";		
		]
	].

