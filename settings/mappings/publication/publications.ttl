@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Publication>
	rml:logicalSource [ rml:source "/mnt/e/data/data/publication/publication_LC.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Publication/{pmid}";
		rr:class clarify:Publication
	];
	rr:predicateObjectMap [
		rr:predicate clarify:title;
		rr:objectMap [ 
			rml:reference "title" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:pubmedID;
		rr:objectMap [ 
			rml:reference "pmid" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:year;
		rr:objectMap [ 
			rml:reference "year";
			rr:datatype xsd:integer; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:journal;
		rr:objectMap [ 
			rml:reference "journal" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:author;
		rr:objectMap [ 
			rml:reference "authors" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:rankingScore_SJR;
		rr:objectMap [ 
			rml:reference "journal_SJR";
			rr:datatype xsd:float; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:rankingScore_hIndex;
		rr:objectMap [ 
			rml:reference "journal_hIndex";
			rr:datatype xsd:integer; 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:publicationType;
		rr:objectMap [ 
			rml:reference "PublicationTypes" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:authorAffiliation;
		rr:objectMap [ 
			rml:reference "Affiliations" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:citedBy;
		rr:objectMap [ 
			rml:reference "citedByCount";
			rr:datatype xsd:float; 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:citedByLink;
		rr:objectMap [ 
			rml:reference "citedByLink" ;
			rr:termType rr:IRI; 
		]
	];		
	rr:predicateObjectMap [
		rr:predicate clarify:publication_isRelatedTo_Disease;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/Disorder/LungCancer"; 
		]
	].	


