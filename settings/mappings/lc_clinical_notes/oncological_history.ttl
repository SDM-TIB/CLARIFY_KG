@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_previousCancerHistory_OrNot;
		rr:objectMap [
			rr:parentTriplesMap <has_previous_cancers>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasPreviousCancerType;
		rr:objectMap [
			rr:parentTriplesMap <previous_cancer_type>
		]
	];					
	rr:predicateObjectMap [
		rr:predicate clarify:description;
		rr:objectMap [
			rml:reference "previous_cancer_description";
		]
	].	

<has_previous_cancers>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/PatientCancerHistory/{has_previous_cancers}";
		rr:class clarify:PatientCancerHistory;
	].

<previous_cancer_type>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/CancerType/{previous_cancer_type}";
		rr:class clarify:CancerType;
	].				
