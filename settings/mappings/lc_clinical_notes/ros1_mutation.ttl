@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/ros1_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <Biomarker>
		]
	].	

<Biomarker>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/ros1_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Biomarker/{description}";
		rr:class clarify:Biomarker;
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerResult>	
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:biomarkerSpecification;
		rr:objectMap [
			rml:reference "specification";		
		]
	].	

<BiomarkerResult>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/ros1_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/BiomarkerResult/{result}";
		rr:class clarify:BiomarkerResult;
	].

