@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{ehr}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasClinicalTrialTreatmentLine;
		rr:objectMap [ 
			rr:parentTriplesMap <ClinicalTrialTreatmentLine>
		]
	].
<ClinicalTrialTreatmentLine>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{ehr}_{treatment_line}";
		rr:class clarify:ClinicalTrialTreatmentLine;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:treatmentLineNumber;
		rr:objectMap [
			rr:parentTriplesMap <TreatmentLineNumber>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasPhase;
		rr:objectMap [
			rr:parentTriplesMap <phase>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasType;
		rr:objectMap [
			rr:parentTriplesMap <targeted_clinical_tr>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasType;
		rr:objectMap [
			rr:parentTriplesMap <therapeutic_target>
		]
	].

<TreatmentLineNumber>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{treatment_line}";
		rr:class clarify:TreatmentLineNumber;
	].

<phase>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <phase_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialPhase";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "phase";		
		]
	].

<targeted_clinical_tr>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <targeted_clinical_tr_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "targeted_clinical_trial";		
		]
	].

<therapeutic_target>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <therapeutic_target_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "therapeutic_target";		
		]
	].

#######################################
############## Functions ##############
#######################################

<phase_Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/clinical_trial_treatment_line.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "clinical_trial_treatment_line" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "phase" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/entity/" 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "phase" 
            ]
        ];                         
    ].	

