@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasStageTNM;
		rr:objectMap [ 
			rr:parentTriplesMap <CancerStageTNM>
		]
	].

<CancerStageTNM>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/CancerStageTNM/{ID}";
		rr:class clarify:CancerStageTNM;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:basedOn_TumorSize;
		rr:objectMap [
			rr:parentTriplesMap <TumorSize>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:basedOn_LymphNodesInvolvement;
		rr:objectMap [
			rr:parentTriplesMap <LymphNodesInvolvement>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:basedOn_MetastatisStage;
		rr:objectMap [
			rr:parentTriplesMap <MetastatisStage>
		]
	]	
	rr:predicateObjectMap [
		rr:predicate clarify:basedOn_Version;
		rr:objectMap [
			rr:parentTriplesMap <TNM_version>
		]
	].	

<TumorSize>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <TumorSize_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TumorSize";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "t";		
		]
	]. 

<LymphNodesInvolvement>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <LymphNodesInvolvement_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/LymphNodesInvolvement";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "n";		
		]
	]. 

<MetastatisStage>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <MetastatisStage_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/MetastatisStage";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "m";		
		]
	].

<TNM_version>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/tnm_stadification.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <TNM_version_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/Version";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "specify_tnm_version";		
		]
	].					