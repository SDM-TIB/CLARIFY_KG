@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<OncologicalSurgery>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/surgery_subtype.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/OncologicalSurgery/{HER}_{treatment_line}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSubType;
		rr:objectMap [
			rr:parentTriplesMap <SurgerySubType>
		]
	];	

<SurgerySubType>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/surgery_subtype.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <SurgerySubType_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/SurgerySubType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "surgery_subtype";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSurgeryIteration;
		rr:objectMap [
			rr:parentTriplesMap <SurgeryIteration>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:surgerySubTypeSpecification;
		rr:objectMap [
			rml:reference "specify_surgery_subtype";		
		]
	].

<SurgeryIteration>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/SurgeryIteration/{surgery_iteration}";
		rr:class clarify:SurgeryIteration;
	].

#######################################
############## Functions ##############
#######################################

<SurgerySubType_Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/surgery.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "surgery_subtype" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "surgery_subtype" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/SurgerySubType" 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "surgery_subtype" 
            ]
        ];                         
    ].