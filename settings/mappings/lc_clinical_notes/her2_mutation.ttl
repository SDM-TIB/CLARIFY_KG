@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <Biomarker>
		]
	].	

<Biomarker>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Biomarker/HER2";
		rr:class clarify:Biomarker;
	];				
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestIHQResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerIHQResult>		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestFISHResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerFISHResult>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_performed_study;
		rr:objectMap [
			rr:parentTriplesMap <study_performed>		
		]
	].

<BiomarkerIHQResult>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <BiomarkerIHQResult_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/BiomarkerTestResult";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "result_her2_ihq";		
		]
	].

<BiomarkerFISHResult>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <BiomarkerFISHResult_Function>
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/BiomarkerTestResult";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "result_her2_fish";		
		]
	].

<study_performed>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <study_performed_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/PerformedStudyStatus";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "study_performed";		
		]
	].	

#######################################
############## Functions ##############
#######################################

<BiomarkerIHQResult_Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/her2_mutation.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "her2_mutation" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "result_her2_ihq" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/BiomarkerTestResult" 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "result_her2_ihq" 
            ]
        ];                         
    ].

