@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/egfr_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <Biomarker>
		]
	].	

<Biomarker>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/egfr_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:constant "http://clarify2020.eu/Biomarker/EGFR";
        rr:termType rr:IRI;
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/Biomarker";
        ]
    ];  	
	rr:predicateObjectMap [
		rr:predicate clarify:hasNegativeTestResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerNegativeResult>		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:has_mutation_T790M_positive_result;
		rr:objectMap [
			rr:parentTriplesMap <T790Result>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:has_mutation_T790M_negative_result;
		rr:objectMap [
			rr:parentTriplesMap <T790MResult>	
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:has_mutation_exon19_result;
		rr:objectMap [
			rr:parentTriplesMap <Exon19Result>	
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:has_mutation_exon21_result;
		rr:objectMap [
			rr:parentTriplesMap <Exon21Result>	
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_NOS_result;
		rr:objectMap [
			rr:parentTriplesMap <NOS_result>		
		]
	];		
	rr:predicateObjectMap [
		rr:predicate clarify:has_other_result;
		rr:objectMap [
			rr:parentTriplesMap <others_result>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_performed_study;
		rr:objectMap [
			rr:parentTriplesMap <study_performed>		
		]
	].

<BiomarkerNegativeResult>
	rml:logicalSource [ rml:source "/mnt/e/data/data/lc_clinical_notes/egfr_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <result_egfr_negative_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/BiomarkerTestResult";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "result_egfr_negative";		
		]
	].	

