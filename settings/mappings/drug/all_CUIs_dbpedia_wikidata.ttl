@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Annotation>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{CUI}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annLabel;
		rr:objectMap [ 
			rml:reference "Label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/UMLS"
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeID;
		rr:objectMap [ 
			rml:reference "type_id" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeLabel;
		rr:objectMap [ 
			rml:reference "Semantic_type_label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeGroup;
		rr:objectMap [ 
			rml:reference "Semantic_group" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <Wikidata>
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <DBpedia>
		]
	].

<Wikidata>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{Wikidata}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "DBpedia_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/Wikidata"
		]
	].

<DBpedia>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{Wikidata}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "Wikidata_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/Wikidata"
		]
	].