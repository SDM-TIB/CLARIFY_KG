@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<DrugDrugPrediction>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/DDI_prediction_LUH.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/DrugDrugPrediction/{Drug1}_interactsWith_{Drug2}";
		rr:class clarify:DrugDrugPrediction
	];
	rr:predicateObjectMap [
		rr:predicate clarify:interactor1;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/Drug/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:interactor2;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/Drug/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:confidence;
		rr:objectMap [ 
			rml:reference "Prob" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:template "http://clarify2020.eu/SourceName/{Source}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:predictionMethod;
		rr:objectMap [ 
			rml:reference "Method" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:methodAbbreviation;
		rr:objectMap [ 
			rml:reference "MethodAbbreviation" 
		]
	].			
		
