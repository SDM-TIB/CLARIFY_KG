@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient_lc_clinical_notes_SLCG_biomarker_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/biomarker_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}";
		rr:class clarify:LCPatient
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <Biomarker_lc_clinical_notes_SLCG_biomarker_mutation>
		]
	].	

<Biomarker_lc_clinical_notes_SLCG_biomarker_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/biomarker_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}_{study_performed}";
		rr:class clarify:BiomarkerObservation ;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:biomarker;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/AlK";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasTestResult;
		rr:objectMap [
			rr:parentTriplesMap <BiomarkerResult_lc_clinical_notes_SLCG_biomarker_mutation>	
		]
	].	

<BiomarkerResult_lc_clinical_notes_SLCG_biomarker_mutation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/biomarker_mutation.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{result}";
		rr:class clarify:BiomarkerTestResult ;

	].		
	
