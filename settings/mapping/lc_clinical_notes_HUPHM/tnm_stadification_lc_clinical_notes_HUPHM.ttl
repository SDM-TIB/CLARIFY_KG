@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient_lc_clinical_notes_HUPHM_tnm_stadification>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/tnm_stadification_join_diagnosis_date.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:LCPatient
	];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTNMstageAssessment;
        rr:objectMap [
            rr:parentTriplesMap <TNM_Stage_assessment_lc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ]. 

<TNM_Stage_assessment_lc_clinical_notes_HUPHM_tnm_stadification>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/tnm_stadification_join_diagnosis_date.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{EHR}_Diagnosis_StageAssessment";
        rr:class clarify:TNMstageAssessment
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:isAssessedDuring;
        rr:objectMap [
            rr:constant "Diagnosis";  
            rr:termType rr:Literal;      
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:isAssessedOn;
        rr:objectMap [
            rml:reference "date";  
            rr:datatype xsd:date ;     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasStage;
        rr:objectMap [
            rml:reference "stage";      
        ]
    ]; 
    rr:predicateObjectMap [
        rr:predicate clarify:hasTNMstage;
        rr:objectMap [
            rr:parentTriplesMap <TNM_Stage_lc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ]. 

<TNM_Stage_lc_clinical_notes_HUPHM_tnm_stadification>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/tnm_stadification_join_diagnosis_date.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{t}_{n}_{m}";
        rr:class clarify:TNMstage
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_TumorSize;
        rr:objectMap [
            rr:parentTriplesMap <TumorSize_lc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_LymphNodesInvolvement;
        rr:objectMap [
            rr:parentTriplesMap <LymphNodesInvolvement_lc_clinical_notes_HUPHM_tnm_stadification>     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_MetastatisStage;
        rr:objectMap [
            rr:parentTriplesMap <MetastatisStage_lc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ]; 	
	rr:predicateObjectMap [
		rr:predicate clarify:cancerStageTNM_Version;
		rr:objectMap [
			rr:parentTriplesMap <TNM_version_lc_clinical_notes_HUPHM_tnm_stadification>
		]
	].	

<TumorSize_lc_clinical_notes_HUPHM_tnm_stadification>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/tnm_stadification_join_diagnosis_date.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <TumorSize_Function_lc_clinical_notes_HUPHM_tnm_stadification> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TumorSize";		
		]
	];
	rr:predicateObjectMap [
