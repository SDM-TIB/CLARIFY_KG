@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<RadiotherapyTreatmentLine_lc_clinical_notes_HUPHM_radiotherapy_drugs>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/radiotherapy_drugs.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}_RadiotherapyTL_{treatment_line}";
		rr:class clarify:RadiotherapyTreatmentLine
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:treatmentLineDosage;
		rr:objectMap [
			rml:reference "dose";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:treatmentLineFractioning;
		rr:objectMap [
			rml:reference "fractioning";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <Annotation_lc_clinical_notes_HUPHM_radiotherapy_drugs>
		]
	].	

<Annotation_lc_clinical_notes_HUPHM_radiotherapy_drugs>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_HUPHM/radiotherapy_drugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{CUI}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:template "http://clarify2020.eu/entity/UMLS"
		]
	].					