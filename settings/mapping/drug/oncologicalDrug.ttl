@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Drug>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/oncologicalDrugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{DrugName}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/vocab/{DrugType}Drug"
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:drugLabel;
		rr:objectMap [
			rml:reference "DrugName";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasCUIAnnotation;
		rr:objectMap [ 
			rr:parentTriplesMap <Annotation>
		]
	].

<Annotation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/oncologicalDrugs.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{CUI}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/UMLS"
		]
	].