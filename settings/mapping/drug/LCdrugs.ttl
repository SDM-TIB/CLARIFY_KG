@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LungCancerDrug>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/LCdrugList.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{DrugBankID}";
		rr:class clarify:LungCancerDrug
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:parentTriplesMap <LungCancerDrugName>
		]
	].	

<LungCancerDrugName>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/LCdrugList.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{DrugName}";
		rr:class clarify:LungCancerDrug
	].
		