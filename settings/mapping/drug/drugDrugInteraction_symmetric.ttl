@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<SymmetricDDI>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/Symmetric_DDI_corpus.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{PrecipitantDrugID}_hasSymmetricInteractionWith_{ObjectDrugID}";
		rr:class clarify:SymmetricDrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:subClassOf;
		rr:objectMap [
			rr:parentTriplesMap <DrugDrugInteraction>
		];
	].	

<DrugDrugInteraction>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/drug/Symmetric_DDI_corpus.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{PrecipitantDrugID}_interactsWith_{ObjectDrugID}";
		rr:class clarify:DrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate clarify:precipitant_drug;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/entity/{PrecipitantDrugID}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:object_drug;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/entity/{ObjectDrugID}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:effect;
		rr:objectMap [
			rr:template "http://clarify2020.eu/entity/{Adverse_Event}_{Impact}"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/DrugBank"
		]
	].

