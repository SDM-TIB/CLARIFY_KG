<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/studied_markers_therapy.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{ehr}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:biomarkerAnalysisDate;
		rr:objectMap [
			rml:reference "analysis_date";
			rr:datatype xsd:date
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasTreatmentLineNumber;
		rr:objectMap [
			rr:parentTriplesMap <TreatmentLineNumber>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasStudiedBiomarker;
		rr:objectMap [ 
			rr:parentTriplesMap <marker_studied>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarkerTestResult;
		rr:objectMap [ 
			rr:parentTriplesMap <egfr_result>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarkerTestResult;
		rr:objectMap [ 
			rr:parentTriplesMap <alk_result>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarkerTestResult;
		rr:objectMap [ 
			rr:parentTriplesMap <kras_result>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasBiomarkerTestResult;
		rr:objectMap [ 
			rr:parentTriplesMap <ros1_result>
		]
	];
	rr:predicateObjectMap [
