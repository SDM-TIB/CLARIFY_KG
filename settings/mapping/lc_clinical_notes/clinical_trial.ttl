@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:involved_in;
		rr:objectMap [ 
			rr:parentTriplesMap <drug_trial>
		]
	].

<drug_trial>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <drug_trial_Function> ;
    rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "drug_trial";		
		]
	];
    rr:predicateObjectMap [
        rr:predicate clarify:hasType;
        rr:objectMap [
            rr:parentTriplesMap <observational_sanitary_others>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:hasPhase;
        rr:objectMap [
            rr:parentTriplesMap <participation_clinical_trial>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:is_directed_OrNot;
        rr:objectMap [
            rr:parentTriplesMap <participation_trial_or_database>
        ]
    ].

<observational_sanitary_others>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <observational_sanitary_others_Function> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/TrialType";        
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "observational_sanitary_others";      
        ]
    ].

<participation_clinical_trial>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <participation_clinical_trial_Function> ;
    rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialParticipation";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "participation_clinical_trial";		
		]
	].

<participation_trial_or_database>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <participation_trial_or_database_Function> ;
    rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TrialParticipation";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "participation_trial_or_database";		
		]
	].

#######################################
############## Functions ##############
#######################################

<observational_sanitary_others_Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/clinical_trial.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "clinical_trial" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "observational_sanitary_others" 
            ]
        ]; 
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/entity/" 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "observational_sanitary_others" 
            ]
        ];                         
    ].	

