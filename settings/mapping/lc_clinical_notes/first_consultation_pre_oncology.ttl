@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/First_consultation_pre_oncology.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasNonOncologicalConsultation;
		rr:objectMap [ 
			rr:parentTriplesMap <NonOncologicalConsultation>
		]
	].

<NonOncologicalConsultation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/First_consultation_pre_oncology.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}_{date_consultation}";
		rr:class clarify:NonOncologicalConsultation;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:date;
		rr:objectMap [
			rml:reference "date_consultation";
			rr:datatype xsd:date;
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:patient_is_asymptomatic;
		rr:objectMap [
			rr:parentTriplesMap <asymptomatic>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <cough>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <pain>
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <dyspnoea>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <hemoptysis>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <asthenia>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <anorexia>		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasSymptom;
		rr:objectMap [
			rr:parentTriplesMap <weightLoss>		
		]
	].	

<asymptomatic>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/First_consultation_pre_oncology.csv";
						rml:referenceFormulation ql:CSV;
						];		
	rr:subjectMap <asymptomatic_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/Phenotype";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "asymptomatic";		
		]
	].

