@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasProgressionRelapse;
		rr:objectMap [
			rr:parentTriplesMap <ProgressionRelapse>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasProgressionType;
		rr:objectMap [
			rr:parentTriplesMap <ProgressionType>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasTherapyType;
		rr:objectMap [
			rr:parentTriplesMap <TherapyType>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:date;
		rr:objectMap [
			rml:reference "occurrence_date";
			rr:datatype xsd:date
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasTreatmentLineNumber;
		rr:objectMap [
			rr:parentTriplesMap <TreatmentLineNumber>
		]
	].		

<ProgressionRelapse>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <ProgressionRelapse_Function> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/ProgressionRelapse";      
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "progression_or_relapse";       
        ]
    ]. 
    
<ProgressionType>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <ProgressionType_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/ProgressionType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "progression_type";		
		]
	]. 

<TherapyType>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap <TherapyType_Function> ;
	rr:termType rr:IRI;

	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:template "http://clarify2020.eu/vocab/TherapyType";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasOriginalValue;
		rr:objectMap [
			rml:reference "therapy_type";		
		]
	]. 	
	
<TreatmentLineNumber>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/TreatmentLineNumber/TL_{treatment_line}";
		rr:class clarify:TreatmentLineNumber;
	].	

#######################################
############## Functions ##############
#######################################

<ProgressionRelapse_Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes/relapse_progression.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant clarifyFun:findSemantic 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:tableName;
            rr:objectMap [ 
                rr:constant "relapse_progression" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnName;
            rr:objectMap [ 
                rr:constant "progression_or_relapse" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate clarifyFun:resource;
            rr:objectMap [ 
                rr:constant "http://clarify2020.eu/entity/" 
            ]
        ];   
        rr:predicateObjectMap [
            rr:predicate clarifyFun:columnValue;
            rr:objectMap [ 
                rml:reference "progression_or_relapse" 
            ]
        ];                         
    ].

