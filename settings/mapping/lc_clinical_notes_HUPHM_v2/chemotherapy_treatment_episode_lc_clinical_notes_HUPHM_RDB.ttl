<LCPatient_episode_cycle_lc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
    rml:logicalSource [ 
        rml:source <RDB_source>;
        rr:sqlVersion rr:SQL2008;
        rml:query """SELECT DISTINCT `chemotherapy_drugs`.`EHR`,
        `chemotherapy_treatment_line`.`start_date`,`chemotherapy_drugs`.`cycle`
        FROM `chemotherapy_drugs`, `chemotherapy_treatment_line` 
        WHERE `chemotherapy_drugs`.`EHR`=`chemotherapy_treatment_line`.`EHR` 
        AND `chemotherapy_drugs`.`treatment_line`=`chemotherapy_treatment_line`.`treatment_line`""";
        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{`chemotherapy_drugs`.`EHR`}_Chemotherapy_{`chemotherapy_treatment_line`.`start_date`}";
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasEpisode;
        rr:objectMap [
            rr:template "http://clarify2020.eu/entity/Cycle_{`chemotherapy_drugs`.`cycle`}";
        ]
    ].

