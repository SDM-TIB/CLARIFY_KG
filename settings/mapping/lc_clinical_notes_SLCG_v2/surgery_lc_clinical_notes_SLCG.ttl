@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient_lc_clinical_notes_SLCG_surgery>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/surgery.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{EHR}";
        rr:class clarify:LCPatient
	];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTreatmentEpisode;
        rr:objectMap [ 
            rr:parentTriplesMap <treatement_episode_lc_clinical_notes_SLCG_surgery>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTNMstageAssessment;
        rr:objectMap [
            rr:parentTriplesMap <TNM_Stage_assessment_lc_clinical_notes_SLCG_surgery>
        ]
    ].


########## treatment episode (surgery) ###########

<treatement_episode_lc_clinical_notes_SLCG_surgery>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/chemotherapy_treatment_line.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{EHR}_Surgery_{surgery_date}";
        rr:class clarify:TreatmentEpisode
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTreatmentType;
        rr:objectMap [
            rr:parentTriplesMap <Treatment_Episode_surgery_lc_clinical_notes_SLCG_surgery>
        ]
    ];     
    rr:predicateObjectMap [
        rr:predicate clarify:hasEpisode;
        rr:objectMap [
            rr:parentTriplesMap <Treatment_Episode_surgery_TreatmentLineNumber_lc_clinical_notes_SLCG_surgery>
        ]
    ].

<Treatment_Episode_surgery_lc_clinical_notes_SLCG_surgery>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/surgery.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{EHR}_OncologicalSurgery_{treatment_line}";
        rr:class clarify:OncologicalSurgery
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:hasPathologicalResponse;
        rr:objectMap [
            rr:parentTriplesMap <SurgeryPathologicalResponse_lc_clinical_notes_SLCG_surgery>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasResectionGrade;
        rr:objectMap [
            rr:parentTriplesMap <SurgeryResectionDegree_lc_clinical_notes_SLCG_surgery>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:has_PerioperativeDeath_orNot;
        rr:objectMap <PerioperativeDeath_Function_lc_clinical_notes_SLCG_surgery> ;
        rr:termType rr:Literal ;
    ];      
    rr:predicateObjectMap [
        rr:predicate clarify:surgeryDate;
        rr:objectMap [
            rml:reference "surgery_date";
            rr:datatype xsd:date;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasType;
        rr:objectMap [
            rr:parentTriplesMap <SurgeryType_lc_clinical_notes_SLCG_surgery>
        ]
    ];      
    rr:predicateObjectMap [
        rr:predicate clarify:hasClass;
        rr:objectMap [
            rr:parentTriplesMap <SurgeryClass_lc_clinical_notes_SLCG_surgery>
        ]
    ].  

<SurgeryPathologicalResponse_lc_clinical_notes_SLCG_surgery>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/surgery.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <SurgeryPathologicalResponse_Function_lc_clinical_notes_SLCG_surgery> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/SurgeryPathologicalResponse";      
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "pathological_response";      
        ]
    ].

