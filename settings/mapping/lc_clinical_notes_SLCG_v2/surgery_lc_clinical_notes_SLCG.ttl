######### tnm stage #################


<TNM_Stage_assessment_lc_clinical_notes_SLCG_surgery>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/surgery.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{EHR}_Diagnosis_{surgery_date}_{stage_t}_{stage_n}_{stage_m}";
        rr:class clarify:TNMstageAssessment
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:isAssessedDuring;
        rr:objectMap [
            rr:constant "Surgery";  
            rr:termType rr:Literal;      
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:isAssessedOn;
        rr:objectMap [
            rml:reference "surgery_date";  
            rr:datatype xsd:date ;     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTNMstage;
        rr:objectMap [
            rr:parentTriplesMap <TNM_Stage_lc_clinical_notes_SLCG_surgery>
        ]
    ]. 

<TNM_Stage_lc_clinical_notes_SLCG_surgery>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/surgery.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{stage_t}_{stage_n}_{stage_m}";
        rr:class clarify:TNMstage
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_TumorSize;
        rr:objectMap [
            rr:parentTriplesMap <TumorSize_lc_clinical_notes_SLCG_surgery>
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_LymphNodesInvolvement;
        rr:objectMap [
            rr:parentTriplesMap <LymphNodesInvolvement_lc_clinical_notes_SLCG_surgery>     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:cancerStageTNM_MetastatisStage;
        rr:objectMap [
            rr:parentTriplesMap <MetastatisStage_lc_clinical_notes_SLCG_surgery>
        ]
    ].  

