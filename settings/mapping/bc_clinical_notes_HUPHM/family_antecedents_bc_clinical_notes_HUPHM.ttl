@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<BCPatient_bc_clinical_notes_HUPHM_family_antecedents>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/family_antecedents.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
        rr:class clarify:BCPatient
	];	
    rr:predicateObjectMap [
        rr:predicate clarify:has_familyWithCancer_OrNot;
        rr:objectMap [
            rr:parentTriplesMap <FamilyWithCancer_bc_clinical_notes_HUPHM_family_antecedents>
        ]
    ]; 
    rr:predicateObjectMap [
        rr:predicate clarify:has_parentWithCancer_OrNot;
        rr:objectMap [
            rr:parentTriplesMap <ParentWithCancer_bc_clinical_notes_HUPHM_family_antecedents>
        ]
    ];   	
	rr:predicateObjectMap [
		rr:predicate clarify:hasFamilyHistory;
		rr:objectMap [ 
			rr:parentTriplesMap <FamilyHistory_bc_clinical_notes_HUPHM_family_antecedents>
		]
	].	

<FamilyWithCancer_bc_clinical_notes_HUPHM_family_antecedents>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/family_antecedents.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <FamilyWithCancer_Function_bc_clinical_notes_HUPHM_family_antecedents> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/FamilyWithCancer";     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "af";      
        ]
    ].

<ParentWithCancer_bc_clinical_notes_HUPHM_family_antecedents>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/family_antecedents.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
