        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/ParentWithCancer";     
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "N1";      
        ]
    ].

<FamilyHistory_bc_clinical_notes_HUPHM_family_antecedents>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/family_antecedents.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}_{family_member}";
		rr:class clarify:FamilyHistory;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:familyType;
		rr:objectMap [
			rr:parentTriplesMap <FamilyType_bc_clinical_notes_HUPHM_family_antecedents>
		]
	];
    rr:predicateObjectMap [
        rr:predicate clarify:familyRelationDegree;
        rr:objectMap [
            rr:parentTriplesMap <FamilyDegree_bc_clinical_notes_HUPHM_family_antecedents>
        ]
    ].

<FamilyType_bc_clinical_notes_HUPHM_family_antecedents>
