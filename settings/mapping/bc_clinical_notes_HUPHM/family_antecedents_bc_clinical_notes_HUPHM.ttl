<FamilyDegree_bc_clinical_notes_HUPHM_family_antecedents>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/family_antecedents.csv";
                        rml:referenceFormulation ql:CSV;
                        ];  
    rr:subjectMap <family_degree_Function_bc_clinical_notes_HUPHM_family_antecedents> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
