@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix clarifyFun: <http://clarify2020.eu/function/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<BCPatient_bc_clinical_notes_HUPHM_tumor_histology>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/tumor_histology.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/HUPHM_{EHR}";
		rr:class clarify:BCPatient
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasTumorHistology;
		rr:objectMap [ 
			rr:parentTriplesMap <TumorHistology_bc_clinical_notes_HUPHM_tumor_histology>
		]
	].

<TumorHistology_bc_clinical_notes_HUPHM_tumor_histology>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/tumor_histology.csv";
						rml:referenceFormulation ql:CSV;
						];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{type}";
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/TumorType";        
        ]
    ].

