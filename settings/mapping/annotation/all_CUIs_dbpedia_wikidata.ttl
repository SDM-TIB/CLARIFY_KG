@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Annotation_label>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{Label}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasCUIAnnotation;
		rr:objectMap [ 
			rr:parentTriplesMap <Annotation>
		]
	].

<Annotation>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{CUI}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annLabel;
		rr:objectMap [ 
			rml:reference "Label" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/UMLS"
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <Wikidata>
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <DBpedia>
		]
	].

<CUI_SemanticType>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/CUI_type.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{cui_id}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeID;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/entity/{type_id}";
		]
	].

<SemanticType>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/Semantic_types.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{Id}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeLabel;
		rr:objectMap [
			rr:template "http://clarify2020.eu/entity/{Label}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeDefition;
		rr:objectMap [ 
			rml:reference "definition" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeGroupID;
		rr:objectMap [ 
			rr:parentTriplesMap <SemanticGroup> ;
			rr:joinCondition [
				rr:parent "Id";
				rr:child "SemanticTypeId";

			];
		];
	].

<SemanticGroup>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/semgroups.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/entity/{Id}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:semanticTypeGroupLabel;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/entity/{Label}"
		]
	].

<Wikidata>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{Wikidata}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "Wikidata_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/Wikidata"
		]
	].

<DBpedia>
	rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/annotation/UMLS_combined.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "{DBpedia}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rml:reference "DBpedia_type" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:has_provenance;
		rr:objectMap [
			rr:constant "http://clarify2020.eu/entity/DBpedia"
		]
	].
