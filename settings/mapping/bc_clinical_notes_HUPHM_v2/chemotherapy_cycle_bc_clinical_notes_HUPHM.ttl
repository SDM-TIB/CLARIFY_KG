<LCPatient_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/chemotherapy_treatment_line.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/HUPHM_{ehr}";
        rr:class clarify:LCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTreatmentEpisode;
        rr:objectMap [ 
            rr:parentTriplesMap <Treatment_Episode_chemotherapy_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
        ]
    ].

### TreatmentType: Chemotherapy, immunotherapy, ...
### EpisodeType: Neoadjuvant or Adjuvant
### Episode: treatment line number, cycle number, or number of cycles
### Type: type of treatment based on the dictionary provided by upm: QT_chemotherapy,...

<Treatment_Episode_chemotherapy_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/chemotherapy_treatment_line.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/{ehr}_Chemotherapy_{date}";
        rr:class clarify:TreatmentEpisode
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasTreatmentType;
        rr:objectMap [
            rr:parentTriplesMap <Treatment_Type_chemotherapy_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
        ]
    ]; 
    rr:predicateObjectMap [
        rr:predicate clarify:hasEpisodeType;
        rr:objectMap [
            rr:parentTriplesMap <Treatment_Episode_chemotherapy_episode_type_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
        ]
    ]; 
    rr:predicateObjectMap [
        rr:predicate clarify:hasBestResponse;
        rr:objectMap [
            rr:parentTriplesMap <TreatementLineBestResponse_bc_clinical_notes_HUPHM_chemotherapy_treatment_line>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:treatementLineStartDate;
        rr:objectMap [
            rml:reference "start_date";
            rr:datatype xsd:date
        ]
    ];  
    rr:predicateObjectMap [
        rr:predicate clarify:treatementLineEndingDate;
        rr:objectMap [
            rml:reference "end_date";
            rr:datatype xsd:date
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasEpisode;
        rr:objectMap [
            rr:parentTriplesMap <episode_TreatmentLineNumber_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasType;
        rr:objectMap [
            rr:parentTriplesMap <Treatment_UPM_type_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
        ]
    ].   

<Treatment_Episode_chemotherapy_episode_type_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/lc_clinical_notes_SLCG/chemotherapy_treatment_line.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap <Treatment_Episode_chemotherapy_episode_type_Function_bc_clinical_notes_HUPHM_chemotherapy_treatment_episode> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://clarify2020.eu/vocab/EpisodeType";       
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasOriginalValue;
        rr:objectMap [
            rml:reference "type";       
        ]
    ].

