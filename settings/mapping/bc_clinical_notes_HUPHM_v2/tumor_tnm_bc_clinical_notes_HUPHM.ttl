<BCPatient_bc_clinical_notes_HUPHM_tnm_stadification>
    rml:logicalSource [ rml:source "/mnt/e/CLARIFY-KG-pipeline/data/bc_clinical_notes_HUPHM/tnm_stadification.csv";
                        rml:referenceFormulation ql:CSV;
                        ];
    rr:subjectMap [
        rr:template "http://clarify2020.eu/entity/HUPHM_{ehr}";
        rr:class clarify:BCPatient
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasDiagnosis;
        rr:objectMap [
            rr:parentTriplesMap <Diagnosis_bc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate clarify:hasNeoadjuvantTherapy;
        rr:objectMap [
            rr:parentTriplesMap <NeoadjuvantTherapy_bc_clinical_notes_HUPHM_tnm_stadification>
        ]
    ].

